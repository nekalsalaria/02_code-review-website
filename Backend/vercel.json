{
    "install":"npm install --legacy-peer-deps",
    "version":2,
    "builds":[
        {
            "src":"server.js",
            "use":"@vercel/node"
        },
        {
            "src":"api/*",
            "use":"@vercel/static"
        }
    ],
    "routes":[
        {
            "src":"/api/.*",
            "dest":"/api/index.js"
        }
    ]
}